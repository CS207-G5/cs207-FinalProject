<?xml version="1.0"?>

<ctml>

    <phase>
        <speciesArray> H O2 O </speciesArray>
    </phase>

    <reactionData id="test_mechanism">

        <!-- reaction 01  -->
        <reaction reversible="no" type="Elementary" id="reaction01">
            <equation>H + O2 [=] OH + O</equation>
            <rateCoeff>
                <k>10</k>
            </rateCoeff>
            <reactants>H:1 O2:2 O:0</reactants>
            <products>O2:1 O:1</products>
        </reaction>

        <!-- reaction 02 -->
        <reaction reversible="no" type="Elementary" id="reaction02">
            <equation>H2 + O [=] OH + H</equation>
            <rateCoeff>
                <k>10</k>
            </rateCoeff>
            <reactants>H:2 O2:0 O:2</reactants>
            <products>O2:1 H:1</products>
        </reaction>

    </reactionData>

</ctml>
